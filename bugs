<script type="text/template" id="gadget">
    <div class="gadget <?if color ?>${color}<?/if?>" id="gadget-${id}-renderbox">
        <div class="dashboard-item-frame gadget-container" id="gadget-${id}-chrome">
            <div class="dashboard-item-header">
                <h3 id="gadget-${id}-title" class="dashboard-item-title"><?if top.titleUrl ?><a href="${titleUrl}">${title}</a><?/if?><?else?>${title}<?/else?></h3>
                <div class="gadget-menu">
                    <ul>
                        <?if layout.writable ?>
                        <li class="aui-icon i-move">${AG.param.get("move")}</li>
                        <?/if?>

                        <?if top.isMaximizable ?>
                        <li>
                            <a href="#" id="gadget-${id}-maximize" class="aui-icon maximize" title="${AG.param.get('maximize')}">${AG.param.get('maximize')}</a>
                        </li>
                        <?/if?>
                        ${dropdown(menu)}
                    </ul>
                </div>
            </div>
            <div class="dashboard-item-content  <?if top.minimized ?> minimization<?/if?>">
                <?if top.userPrefs ?>
                <form action="${userPrefs.action}" class="aui userpref-form hidden" method="post" id="gadget-${id}-edit">
                    <fieldset>
                        <legend><span class="dashboard-item-title">${title}</span> ${AG.param.get("preferences")}</legend>
                        <?foreach field in userPrefs.fields?>
                        <?if field.type === "hidden" ?>
                        <input id="gadget-${id}-form-${field.name}-pref" type="hidden" class="hidden" ${attr(field)} />
                        <?/if?>

                        <?elseif field.type === "list" ?>
                        <div id="up_${field.name}-container" class="list-container field-group">
                            <label for="gadget-${id}-form-${field.name}-pref">${field.displayName}</label>
                            <input id="gadget-${id}-form-${field.name}-pref" type="text" class="list text med" ${attr(field)} />
                            <?if field.required ?>field.required<?/if?>
                            <div class="description">${AG.param.get("pipeDelimitedHelp")}</div>
                        </div>
                        <?/elseif?>

                        <?elseif field.type === "bool" ?>
                        <div class="checkbox field-group">
                            <input type="checkbox" value="true" class="bool" id="gadget-${id}-form-${field.name}-pref" <?if field.value === "true" ?>checked<?/if?> ${attr(field)} />
                            <label for="gadget-${id}-form-${field.name}-pref">${field.displayName} <?if field.required ?>field.required<?/if?></label><br/>
                        </div>
                        <?/elseif?>

                        <?elseif field.type === "enum" ?>
                        <div class="field-group">
                            <label for="gadget-${id}-form-${field.name}-pref">${field.displayName} <?if field.required ?>field.required<?/if?></label>
                            <select id="gadget-${id}-form-${field.name}-pref" class="enum" ${attr(field)}>
                            <?foreach option in field.options?>
                            <option class="enum" ${attr(option)}>${option.label}</option>
                            <?/foreach?>
                            </select>
                        </div>
                        <?/elseif?>

                        <?elseif field.type === "string" ?>
                        <div class="field-group">
                            <label for="gadget-${id}-form-${field.name}-pref">${field.displayName}</label>
                            <input id="gadget-${id}-form-${field.name}-pref" type="text" ${attr(field)} class="string text">
                            <?if field.required ?>field.required<?/if?><br/>
                        </div>
                        <?/elseif?>

                        <?/foreach?>
                    </fieldset>
                    <div class="buttons-container submit">
                        <input type="submit" class="submit" value="${AG.param.get('save')}" name="save" id="gadget-${id}-save"/>
                        <input type="reset" class="cancel" value="${AG.param.get('cancel')}" name="reset" id="gadget-${id}-cancel" class="userpref-form-cancel"/>
                    </div>
                </form>
                <?/if?>
                <iframe id="gadget-${id}" class="gadget-iframe" name="gadget-${id}" src="${renderedGadgetUrl}" height="${top.height || 300}" width="100%" scrolling="no" frameborder="no"></iframe>
            </div>
        </div>
    </div>
</script>

